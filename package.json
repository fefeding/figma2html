{
  "name": "j-figma2html",
  "version": "1.0.15",
  "description": "figma 转 html 工具类",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "node test/dev",
    "build": "gulp",
    "build:esm": "node ./build/build.js",
    "api:json": "npx api-extractor run",
    "api:md": "npx api-documenter markdown -i ./docs/.api -o ./docs/api && node ./build/md.js",
    "api": "npm run api:json && npm run api:md",
    "push": "npm publish --registry=https://registry.npmjs.org"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-function-bind": "^7.23.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-stage-0": "^7.8.3",
    "@microsoft/api-documenter": "^7.23.20",
    "@microsoft/api-extractor": "^7.39.4",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/node": "^20.11.30",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "express": "^4.18.2",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-typescript": "^6.0.0-alpha.1",
    "rollup": "^4.9.6",
    "rollup-plugin-typescript2": "^0.36.0",
    "tsify": "^5.0.4",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vinyl-source-stream": "^2.0.0",
    "vitepress": "^1.0.0-rc.40",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "j-design-util": "^1.0.35"
  },
  "author": "fefeding",
  "license": "MIT"
}
